
<h1>Wiki Topics</h1>
<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
    <% if !(wiki.private?) || (current_user.admin? || current_user.premium?)%>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to markdown(wiki.title), wiki %>
          </h4>
        </div>
      </div>
    <% end %>
    <% end %>
  </div>
