
<div class="row">
  <div class="col-md-8">
  <h1>Wiki Topics</h1>
  <h4><em>Click a title to start editing now!</em></h4>
    <% @wikis.each do |wiki| %>
    <% if !(wiki.private?) || (current_user.admin? || current_user.premium?)%>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, edit_wiki_path(wiki) %>
          </h4>
        </div>
      </div>
    <% end %>
    <% end %>
  </div>
